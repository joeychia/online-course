{
  "name": "online-course-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "deploy": "tsc -b && vite build && firebase deploy --only hosting",
    "seed": "tsx src/scripts/seed.ts",
    "restore": "tsx src/scripts/restoreFirestoreAdmin.ts",
    "backup": "tsx src/scripts/backupCourse.ts",
    "test": "vitest",
    "test:once": "vitest run",
    "test:auto": "vitest run --reporter=tap-flat",
    "pre-commit": "npm run test:once && npm run build"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "@toast-ui/react-editor": "^3.2.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-calendar-heatmap": "^1.9.0",
    "analytics": "^0.8.16",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "firebase": "^10.8.0",
    "gray-matter": "^4.0.3",
    "opencc-js": "^1.0.5",
    "react-beautiful-dnd": "^13.1.1",
    "react-calendar-heatmap": "^1.9.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.22.0",
    "react-tooltip": "^4.5.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.6.0",
    "@types/firebase": "^2.4.32",
    "@types/node": "^22.10.5",
    "@types/react": "^17.0.75",
    "@types/react-dom": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "firebase-admin": "^13.0.2",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.2"
  }
}
